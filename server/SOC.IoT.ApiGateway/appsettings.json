{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MqttUrl": "<MQTT_URL>",
  "ConnectionStrings": {
    "Db": "Server=<SOC_CONDUCTOR_DBHOST>;Port=<SOC_CONDUCTOR_DBPORT>;Database=<SOC_CONDUCTOR_DBNAME>;Username=<SOC_CONDUCTOR_DBUSER>;Password=<SOC_CONDUCTOR_DBPASSWORD>"
  },
  "Jwt": {
    "Issuer": "<Issuer>",
    "Audience": "<Audience>",
    "Key": "<Key>",
    "Minutes": 20
  }
}